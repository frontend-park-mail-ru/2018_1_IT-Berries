- let i = 0; let j = 0;
section.view.game-view
    header.view__header.view-header
        div.view-header__home
            a(href='/')
        div.view-header__head Cath the Alien!
        div.view-header__profile Guest
            a(href='/profile' class='view-header__profile-icon profile-icon')
    article
        div.view__content
            div(class='end-game-panel')
                div.end-game-panel__tiitle Game over
                input(type='button' class='end-game-panel__button' value='ok')
            div.players-tables
                each val in [1, 2]
                    table(class='player-panel player-' + val + '-panel')
                        thead
                            tr
                                th(class='player-nickname') Player #{val}
                        tbody
                            tr
                                td Moves:
                                td.player-moves 0
                            tr
                                td Score:
                                td.player-score 0
                    if val==1
                        div.stretching
            div.game-view__game

    footer
        div.footer__fore